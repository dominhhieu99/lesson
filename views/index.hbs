<tbody>
  {{#each products}}
            <tr class="table-success" >
                <td>{{this._id}}</td>
                <td>{{this.cate_id}}</td>
                <td>{{this.name}}</td>
                <td>{{this.price}}</td>
                <td>
                    <img src="{{this.image}}" class="img-responsive" width="100">
                </td>
                <td>{{this.detail}}</td>
                <td>
                    <a href="/products/edit/{{this._id}}" class="btn btn-success">Update</a>
                    <a href="/products/remove/{{this._id}}" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        {{/each}}
</tbody>
</table>
